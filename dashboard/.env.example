# server
PORT=3000

# openrouter (routes to gemini, claude, llama)
# get key at https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# elevenlabs (sfx + tts + voice cloning)
# get key at https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# 3d model generation providers
# meshy: https://app.meshy.ai/settings/api-keys
MESHY_API_KEY=your_meshy_api_key_here
# tripo: https://platform.tripo3d.ai/api-keys
TRIPO_API_KEY=your_tripo_api_key_here
# rodin: https://developer.hyper3d.ai/
RODIN_API_KEY=your_rodin_api_key_here

# roblox open cloud (asset upload)
# create at https://create.roblox.com/credentials (needs Assets scope)
ROBLOX_API_KEY=your_roblox_api_key_here

# mongodb atlas
# get connection string at https://cloud.mongodb.com
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/roblox-dashboard?retryWrites=true&w=majority

# solana (optional, devnet only)
# generate wallet with: solana-keygen new
# airdrop devnet sol with: solana airdrop 2
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_PRIVATE_KEY=your_devnet_private_key_here

# project paths for docs tab (comma-separated)
PROJECT_PATHS=/path/to/project1,/path/to/project2
