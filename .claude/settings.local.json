{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(move \"BlueprintShopService.server.lua\" \"BlueprintShopService.lua\")",
      "Bash(mv:*)",
      "Bash(find:*)",
      "WebFetch(domain:madstudioroblox.github.io)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:github.com)",
      "Bash(mkdir:*)",
      "Bash(rm:*)",
      "Bash(cat:*)",
      "Bash(rg:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "WebFetch(domain:create.roblox.com)",
      "Bash(lua:*)",
      "Bash(dir /s /b:*)",
      "Bash(findstr:*)"
    ],
    "deny": []
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -Command \"[System.Console]::Beep(800, 200); [System.Console]::Beep(800, 200); [System.Console]::Beep(800, 200)\""
          }
        ]
      }
    ]
  },
  "outputStyle": "Explanatory"
}
